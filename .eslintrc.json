{
  "extends": [
    "oclif",
    "oclif-typescript",
    "prettier"
  ],
  "rules": {
    "perfectionist/sort-classes": "off",
    "unicorn/no-for-loop": "off",
    "valid-jsdoc": "off",
    "n/no-process-exit": "off",
    "unicorn/no-process-exit": "off",
    "no-await-in-loop": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "unicorn/prefer-ternary": "off"
  }
}