{
  "name": "wlin-cli",
  "version": "0.3.12",
  "description": "Quickly create common templates",
  "keywords": [
    "oclif",
    "wlin"
  ],
  "homepage": "https://github.com/imehc/wlin-cli",
  "bugs": "https://github.com/imehc/wlin-cli/issues",
  "repository": "https://github.com/imehc/wlin-cli",
  "license": "MIT",
  "author": "imehc",
  "type": "module",
  "exports": "./lib/index.js",
  "main": "",
  "types": "dist/index.d.ts",
  "bin": {
    "wlin-cli": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "build:obfuscator": "pnpm run build && javascript-obfuscator dist/commands/create.js --output dist/commands/create.js",
    "lint": "eslint . --ext .ts",
    "lint:lint-staged": "lint-staged",
    "prepack": "npm run build && npx oclif manifest && npx oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "husky",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "version": "npx oclif readme && git add README.md",
    "pub": "sh version.sh",
    "commit": "git cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "oclif": {
    "bin": "wlin-cli",
    "commands": "./dist/commands",
    "dirname": "wlin-cli",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-commands",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "create": {
        "description": "Create a new project"
      }
    }
  },
  "dependencies": {
    "@oclif/core": "^4.0.9",
    "@oclif/plugin-autocomplete": "^3.1.6",
    "@oclif/plugin-commands": "^4.0.6",
    "@oclif/plugin-help": "^6.2.5",
    "@oclif/plugin-not-found": "^3.2.10",
    "@oclif/plugin-update": "^4.4.7",
    "@oclif/plugin-warn-if-update-available": "^3.1.8",
    "chalk": "^5.3.0",
    "figlet": "^1.7.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.23",
    "node-notifier": "^10.0.1",
    "ora": "^8.0.1",
    "rimraf": "^6.0.0",
    "simple-git": "^3.25.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4.0.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/chai": "^4.3.16",
    "@types/figlet": "^1.5.8",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.10",
    "@types/node-notifier": "^8.0.5",
    "chai": "^5.1.1",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^5.0.0",
    "cz-git": "^1.9.3",
    "eslint": "^8.57.0",
    "eslint-config-oclif": "^5.2.0",
    "eslint-config-oclif-typescript": "^3.1.8",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "javascript-obfuscator": "^4.1.1",
    "lint-staged": "^15.2.7",
    "mocha": "^10.6.0",
    "prettier": "^3.3.2",
    "semantic-release": "^24.0.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  }
}
