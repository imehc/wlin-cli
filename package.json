{
  "name": "wlin-cli",
  "version": "0.3.8",
  "description": "Quickly create common templates",
  "keywords": [
    "oclif",
    "wlin"
  ],
  "homepage": "https://github.com/imehc/wlin-cli",
  "bugs": "https://github.com/imehc/wlin-cli/issues",
  "repository": "https://github.com/imehc/wlin-cli",
  "license": "MIT",
  "author": "imehc",
  "type": "module",
  "exports": "./lib/index.js",
  "main": "",
  "types": "dist/index.d.ts",
  "bin": {
    "wlin-cli": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "build:obfuscator": "pnpm run build && javascript-obfuscator dist/commands/create.js --output dist/commands/create.js",
    "lint": "eslint . --ext .ts",
    "prepack": "npm run build && npx oclif manifest && npx oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "version": "npx oclif readme && git add README.md",
    "pub": "sh version.sh"
  },
  "oclif": {
    "bin": "wlin-cli",
    "commands": "./dist/commands",
    "dirname": "wlin-cli",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-commands",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "create": {
        "description": "Create a new project"
      }
    }
  },
  "dependencies": {
    "@oclif/core": "^3.26.3",
    "@oclif/plugin-autocomplete": "^3.0.17",
    "@oclif/plugin-commands": "^3.3.2",
    "@oclif/plugin-help": "^6.0.21",
    "@oclif/plugin-not-found": "^3.1.8",
    "@oclif/plugin-update": "^4.2.10",
    "@oclif/plugin-warn-if-update-available": "^3.0.15",
    "chalk": "^5.3.0",
    "figlet": "^1.7.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.18",
    "node-notifier": "^10.0.1",
    "ora": "^8.0.1",
    "rimraf": "^5.0.5",
    "simple-git": "^3.24.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3.2.13",
    "@types/chai": "^4.3.16",
    "@types/figlet": "^1.5.8",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.10",
    "@types/node-notifier": "^8.0.5",
    "chai": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-config-oclif": "^5.1.2",
    "eslint-config-oclif-typescript": "^3.1.5",
    "eslint-config-prettier": "^9.1.0",
    "javascript-obfuscator": "^4.1.0",
    "mocha": "^10.4.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
